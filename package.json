{
  "name": "minesweeper",
  "version": "0.1.1",
  "private": true,
  "sideEffects": false,
  "homepage": "https://lucas98sf.github.io/react-minesweeper",
  "type": "module",
  "scripts": {
    "build": "relay-compiler --validate && remix vite:build",
    "dev": "remix vite:dev",
    "preview": "vite preview",
    "lint": "bunx @biomejs/biome check --apply-unsafe .",
    "deploy:pages": "gh-pages -d dist",
    "deploy": "bun run build && bun run deploy:pages",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "relay": "relay-compiler"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/node": "^2.7.2",
    "@remix-run/react": "^2.7.2",
    "@remix-run/serve": "^2.7.2",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "isbot": "^4.1.0",
    "lucide-react": "^0.338.0",
    "nanoid": "^5.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-supabase": "^0.2.0",
    "relay-runtime": "^16.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-supabase": "^4.0.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@remix-run/dev": "^2.7.2",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^0.29.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": "bunx @biomejs/biome check --apply",
    "*.{js,css,md}": "bunx @biomejs/biome check --apply"
  }
}